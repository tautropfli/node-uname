{
  "name": "node-uname",
  "version": "3.1.2",
  "description": "Wrapper around the uname call",
  "type": "module",
  "main": "uname.cjs",
  "exports": {
    ".": {
      "import": "./uname.js",
      "require": "./uname.cjs",
      "types": "./uname.d.ts"
    }
  },
  "dependencies": {
    "bindings": "^1.3"
  },
  "devDependencies": {
    "mocha": "^11.1",
    "node-gyp": "^12.1",
    "rollup": "^4.14"
  },
  "scripts": {
    "install": "node-gyp rebuild",
    "test": "mocha",
    "build": "node-gyp rebuild",
    "prepack": "rollup uname.js --file uname.cjs --format cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tautropfli/node-uname.git"
  },
  "keywords": [
    "uname",
    "linux",
    "darwin"
  ],
  "author": "Tau [https://tau.garden]",
  "license": "MIT",
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/tautropfli/node-uname/issues"
  }
}
